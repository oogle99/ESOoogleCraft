                            <Anchor point="TOPLEFT" relativeTo="$(parent)" offsetX="5" offsetY="325" />
                        </Texture>

                        <Label name="$(parent)ArmorEnchantLabel" font="ZoFontKeyboard18ThickOutline" inheritAlpha="true" color="FFFFFF"

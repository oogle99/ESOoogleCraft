                            <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="1007" offsetY="375" />
                        </Label>

                        <Texture name="$(parent)JewelryEnchantUnderscore" textureFile="esoui\art\deathrecap\deathrecap_divider_right.dds" tier="HIGH">
